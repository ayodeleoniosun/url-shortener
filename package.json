{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "A url shortener app",
  "scripts": {
    "test": "NODE_ENV=test jest --forceExit --coverage --verbose --colors",
    "start": "npm run build && node dist/app.js",
    "migrate": "sequelize db:migrate",
    "watch-ts": "tsc -w",
    "dev": "concurrently \"npm run watch-ts\" \"nodemon dist/app.js\"",
    "build": "rimraf ./build && tsc"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayodeleoniosun/shortener.git"
  },
  "author": "Oniosun Ayodele",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ayodeleoniosun/shortener/issues"
  },
  "homepage": "https://github.com/ayodeleoniosun/shortener#readme",
  "dependencies": {
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "geoip-lite": "^1.4.5",
    "http-status": "^1.5.0",
    "mongoose": "^6.2.10",
    "mysql2": "^2.2.5",
    "redis": "^3.1.2",
    "sequelize": "^6.6.4",
    "sequelize-cli": "^6.3.0",
    "sequelize-typescript": "^2.1.0",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^27.0.1",
    "@types/node": "^15.0.1",
    "@types/sequelize": "^4.28.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^27.0.7",
    "jest-cli": "^26.6.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.2"
  }
}
